  "grammar": [
    {
      "id": 16,
      "title": "形容词/副词比较级",
      "keywords": [
        "比较级",
        "more",
        "-er",
        "than",
        "comparative",
        "比较"
      ],
      "definition": "用于两者之间的比较，表示'更...'的意思。",
      "structure": "形容词/副词比较级 + than / more + 形容词/副词原级 + than",
      "usage": [
        "单音节和部分双音节词：加-er（tall→taller, big→bigger）",
        "多音节和部分双音节词：more + 原级（beautiful→more beautiful）",
        "不规则变化：good/well→better, bad/badly→worse, many/much→more",
        "比较级的修饰词：much, a lot, even, still, a little"
      ],
      "mistakes": [
        {
          "wrong": "She is more taller than me.",
          "correct": "She is taller than me.",
          "explanation": "tall是单音节词，用-er形式，不需要more"
        },
        {
          "wrong": "He runs more fast than me.",
          "correct": "He runs faster than me.",
          "explanation": "fast的比较级是faster，不是more fast"
        },
        {
          "wrong": "This book is more better.",
          "correct": "This book is better / much better.",
          "explanation": "good的比较级是better，不能用more修饰"
        }
      ],
      "examples": [
        "My sister is two years older than me.",
        "This question is more difficult than that one.",
        "He works much harder than before."
      ],
      "sub_topics": [
        {
          "title": "比较级递增结构（more and more）",
          "source_type": "unmatched",
