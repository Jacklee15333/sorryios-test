  "grammar": [
    {
      "title": "动名词作主语",
      "definition": "动词-ing形式可以作主语，表示一件事或一种行为",
      "structure": "doing + sth. + 谓语",
      "usage": [
        "表示抽象行为",
        "谓语动词用第三人称单数"
      ],
      "mistakes": [
        {
          "wrong": "Protect the environment become important.",
          "correct": "Protecting the environment becomes important."
        }
      ],
      "examples": [
        "Protecting the environment becomes more and more important."
      ]
    },
    {
      "title": "主谓一致",
      "definition": "主语为单数概念时，谓语动词用第三人称单数形式",
      "structure": "单数主语 + v.-s",
      "usage": [
        "动名词短语作主语视为单数"
      ],
      "mistakes": [
        {
          "wrong": "Protecting the environment become important.",
          "correct": "Protecting the environment becomes important."
        }
      ],
      "examples": [
        "Doing exercise is good for health."
      ]
    },
    {
      "title": "并列句与连接词",
      "definition": "英语中两句话需要用连接词连接，不能只用逗号",
      "structure": "句子 + and/but/so + 句子",
      "usage": [
        "避免逗号拼接句子",
        "保证每个句子有主语"
      ],
      "mistakes": [
        {
          "wrong": "We should protect the environment, make our life better.",
          "correct": "We should protect the environment, and it can make our life better."
        }
      ],
      "examples": [
        "We should protect the environment, and it can make our life better."
      ]
    },
    {
      "title": "tell / talk / speak 的区别",
      "definition": "三者都与“说”有关，但用法和搭配不同",
      "structure": "tell sb. sth. / tell sb. to do sth.",
      "usage": [
        "tell 强调传递信息",
        "talk 强调交谈",
        "speak 强调说话能力或正式表达"
      ],
      "mistakes": [
        {
          "wrong": "tell sb. doing sth.",
          "correct": "tell sb. to do sth."
        }
      ],
      "examples": [
        "The teacher told us to finish the homework.",
        "I talked with my friend.",
        "He can speak English well."
      ]
    },
    {
      "title": "过去式的用法",
      "definition": "表示过去某个时间发生的动作",
      "structure": "动词过去式",
      "usage": [
        "表示昨天、以前发生的事情"
      ],
      "mistakes": [
        {
          "wrong": "I eat rice yesterday.",
          "correct": "I ate rice yesterday."
        }
      ],
      "examples": [
        "I did my homework last night."
      ]
    },
    {
      "title": "过去分词的三种用法",
      "definition": "过去分词可用于完成时、被动语态和形容词",
      "structure": "have/has + done；be + done",
      "usage": [
        "现在完成时",
        "被动语态",
        "作形容词"
      ],
      "mistakes": [
        {
          "wrong": "I have eat dinner.",
          "correct": "I have eaten dinner."
        }
      ],
      "examples": [
        "I have finished my work.",
        "The apple is eaten by sb.",
        "The eaten apple is on the table."
      ]
    },
    {
      "title": "被动语态",
      "definition": "表示主语是动作的承受者",
      "structure": "be + 过去分词",
      "usage": [
        "不知道或不需要说明动作执行者",
        "强调动作承受者"
      ],
      "mistakes": [
        {
          "wrong": "The house controls by a system.",
          "correct": "The house is controlled by a system."
        }
      ],
      "examples": [
        "The room is cleaned every day."
      ]
    },
    {
      "title": "不规则动词变化",
      "definition": "部分动词过去式和过去分词形式不规则",
      "structure": "原形 / 过去式 / 过去分词",
      "usage": [
        "需要单独记忆"
      ],
      "mistakes": [
        {
          "wrong": "speak / speaked / speaked",
          "correct": "speak / spoke / spoken"
        }
      ],
      "examples": [
        "cut / cut / cut",
        "read / read / read",
        "cost / cost / cost"
      ]
    },
    {
      "title": "介词后接名词或动名词",
      "definition": "介词后面不能直接接动词原形，只能接名词或动名词",
      "structure": "prep. + n./doing",
      "usage": [
        "without doing sth.",
        "in doing sth."
      ],
      "mistakes": [
        {
          "wrong": "without do homework",
          "correct": "without doing homework"
        }
      ],
      "examples": [
        "He left without telling anyone."
      ]
    },
    {
      "title": "It 作形式主语",
      "definition": "用 it 作形式主语，把真正的主语放在后面",
      "structure": "It is + adj. + to do sth.",
      "usage": [
        "避免句子头重脚轻"
      ],
      "mistakes": [
        {
          "wrong": "To speak fluently is surprising.",
          "correct": "It is surprising to speak fluently."
        }
      ],
      "examples": [
        "It is surprising to see a little boy speak fluently."
      ]
    },
    {
      "title": "invite / ask 后接不定式",
      "definition": "invite 和 ask 后只能接 to do，不接 doing",
      "structure": "invite/ask + sb. + to do sth.",
      "usage": [
        "表示尚未发生的动作"
      ],
      "mistakes": [
        {
          "wrong": "invite sb. doing sth.",
          "correct": "invite sb. to do sth."
        }
      ],
      "examples": [
        "They invited him to give a speech."
      ]
    },
    {
      "title": "不定式用法",
      "definition": "to do 表示目的或将要发生的动作，不能用 to doing",
      "structure": "to + 动词原形",
      "usage": [
        "plan to do sth.",
        "want to do sth."
      ],
      "mistakes": [
        {
          "wrong": "want to doing",
          "correct": "want to do"
        }
      ],
      "examples": [
        "She wants to improve her English."
      ]
    },
    {
      "title": "一般疑问句的构成",
      "definition": "一般疑问句需要借助助动词或 be 动词提前",
      "structure": "Do/Does/Did + 主语 + 动词原形",
      "usage": [
        "动词原形句需加 do/does/did",
        "be 动词句直接提前"
      ],
      "mistakes": [
        {
          "wrong": "You like English?",
          "correct": "Do you like English?"
        }
      ],
      "examples": [
        "Does he work here?"
      ]
    },
    {
      "title": "第三人称单数",
      "definition": "主语为第三人称单数时，谓语动词用第三人称单数形式",
      "structure": "第三人称单数 + v.-s",
      "usage": [
        "he/she/it",
        "单数名词"
