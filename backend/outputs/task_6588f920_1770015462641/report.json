  "grammar": [
    {
      "id": 18,
      "title": "感叹句",
      "keywords": [
        "感叹句",
        "what",
        "how",
        "感叹",
        "exclamatory sentence"
      ],
      "definition": "用来表达强烈感情的句子，句末用感叹号。常用What和How引导。",
      "structure": "What + (a/an) + 形容词 + 名词 + 主语 + 谓语! / How + 形容词/副词 + 主语 + 谓语!",
      "usage": [
        "What + a/an + 形容词 + 可数名词单数：What a beautiful girl she is!",
        "What + 形容词 + 不可数名词/复数名词：What nice weather! What beautiful flowers!",
        "How + 形容词/副词：How beautiful the girl is! How fast he runs!",
        "判断用What还是How：看中心词是名词还是形容词/副词"
      ],
      "mistakes": [
        {
          "wrong": "What beautiful girl she is!",
          "correct": "What a beautiful girl she is!",
          "explanation": "可数名词单数前要加a/an"
        },
        {
          "wrong": "How a beautiful girl!",
          "correct": "What a beautiful girl! / How beautiful the girl is!",
          "explanation": "How后直接加形容词，不加冠词和名词"
        },
        {
          "wrong": "What nice the weather is!",
          "correct": "How nice the weather is! / What nice weather!",
          "explanation": "What后要有名词，How后直接加形容词"
        }
      ],
      "examples": [
        "What an interesting story it is!",
        "What delicious food!",
        "How time flies!"
      ],
