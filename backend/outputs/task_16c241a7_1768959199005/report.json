  "grammar": [
    {
      "id": 10,
      "title": "动名词 (v.-ing作名词)",
      "keywords": [
        "动名词",
        "doing",
        "v.-ing",
        "gerund",
        "动词-ing形式"
      ],
      "definition": "由动词-ing形式构成，具有名词性质，可以作主语、宾语、表语等。",
      "structure": "动词-ing",
      "usage": [
        "作主语：Swimming is good for health.",
        "作宾语：I enjoy reading books. / Thank you for helping me.",
        "作表语：My hobby is collecting stamps.",
