  "grammar": [
    {
      "title": "动名词作主语",
      "definition": "动词-ing形式可以作主语，表示一件事情或一种行为",
      "structure": "doing + 谓语",
      "usage": [
        "表示抽象或习惯性的动作",
        "谓语动词用第三人称单数"
      ],
      "mistakes": [
        {
          "wrong": "Protect the environment become important.",
          "correct": "Protecting the environment becomes important."
        }
      ],
      "examples": [
        "Protecting the environment is important."
      ]
    },
    {
      "title": "主谓一致",
      "definition": "谓语动词在人称和数上必须与主语保持一致",
      "structure": "单数主语 + 动词第三人称单数",
      "usage": [
        "动名词作主语视为单数",
        "不可数概念作主语视为单数"
      ],
      "mistakes": [
        {
          "wrong": "Protecting the environment become important.",
          "correct": "Protecting the environment becomes important."
        }
      ],
      "examples": [
        "Saving energy helps the earth."
      ]
    },
    {
      "title": "连接词 and 的使用",
      "definition": "连接词用于连接两个并列的句子或成分",
      "structure": "句子 + and + 句子",
      "usage": [
        "避免逗号连接两个完整句子",
        "保证后一句有明确主语"
      ],
      "mistakes": [
        {
          "wrong": "We should protect the environment, make our life better.",
          "correct": "We should protect the environment, and it makes our life better."
        }
      ],
      "examples": [
        "We save energy and we plant trees."
      ]
    },
    {
      "title": "tell / talk / speak 用法辨析",
      "definition": "三者都与“说话”有关，但搭配和用法不同",
      "structure": "",
      "usage": [
        "tell sb. sth.",
        "tell sb. to do sth.",
        "talk about sth.",
        "talk with/to sb.",
        "speak to sb."
      ],
      "mistakes": [
        {
