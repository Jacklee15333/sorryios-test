  "grammar": [
    {
      "id": 9,
      "title": "不定式 (to + 动词原形)",
      "keywords": [
        "不定式",
        "to do",
        "to + 动词原形",
        "infinitive",
        "动词不定式"
      ],
      "definition": "由to + 动词原形构成的非谓语动词形式，可以作主语、宾语、表语、定语、状语和宾语补足语。",
      "structure": "to + 动词原形",
      "usage": [
        "作宾语：I want to go home. / I decide to study hard.",
        "作目的状语：I came here to see you.",
        "作主语：To learn English well is important. = It is important to learn English well.",
        "作宾语补足语：My mother asked me to clean the room.",
        "作定语：I have something to tell you."
      ],
      "mistakes": [
        {
          "wrong": "I want go home.",
          "correct": "I want to go home.",
          "explanation": "want后接不定式，必须有to"
        },
        {
          "wrong": "He made me to cry.",
          "correct": "He made me cry.",
          "explanation": "make/let/have等使役动词后用省略to的不定式"
        },
        {
          "wrong": "I saw him to run.",
          "correct": "I saw him run/running.",
          "explanation": "感官动词see/hear/watch后用省略to的不定式或-ing"
        }
      ],
      "examples": [
        "She decided to become a doctor.",
        "It's kind of you to help me.",
        "I don't know what to do."
      ],
      "category": "其他",
      "difficulty": 2,
      "enabled": true,
      "created_at": "2026-01-14 02:03:24",
      "updated_at": "2026-01-14 02:03:24",
      "_source": "database",
      "_matchScore": 0.92
    },
    {
      "id": 10,
      "title": "动名词 (v.-ing作名词)",
      "keywords": [
        "动名词",
        "doing",
        "v.-ing",
        "gerund",
        "动词-ing形式"
      ],
      "definition": "由动词-ing形式构成，具有名词性质，可以作主语、宾语、表语等。",
      "structure": "动词-ing",
      "usage": [
        "作主语：Swimming is good for health.",
        "作宾语：I enjoy reading books. / Thank you for helping me.",
        "作表语：My hobby is collecting stamps.",
        "某些动词后只能接动名词：enjoy, finish, mind, practice, avoid, consider等"
      ],
      "mistakes": [
        {
          "wrong": "I enjoy to swim.",
          "correct": "I enjoy swimming.",
          "explanation": "enjoy后只能接动名词，不能接不定式"
        },
        {
          "wrong": "Thank you for help me.",
          "correct": "Thank you for helping me.",
          "explanation": "介词后要用动名词"
        }
      ],
      "examples": [
        "Reading makes a full man.",
        "Would you mind opening the window?",
        "She is good at playing the piano."
      ],
      "category": "其他",
      "difficulty": 2,
      "enabled": true,
      "created_at": "2026-01-14 02:03:24",
      "updated_at": "2026-01-14 02:03:24",
