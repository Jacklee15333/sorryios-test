  "grammar": [
    {
      "id": 4,
      "title": "现在进行时",
      "keywords": [
        "现在进行时",
        "be doing",
        "am/is/are",
        "v.-ing",
        "现在分词",
        "进行时",
        "present continuous",
        "present progressive"
      ],
      "definition": "表示现在正在进行的动作，也可表示现阶段正在进行或即将发生的动作。",
      "structure": "主语 + am/is/are + 动词-ing",
      "usage": [
        "表示说话时正在进行的动作：He is reading a book now.",
        "表示现阶段正在进行的动作：She is learning French this year.",
        "表示即将发生的动作（常用go, come, leave等）：I am leaving tomorrow.",
        "与always连用表示赞扬或不满：He is always helping others."
      ],
      "mistakes": [
        {
          "wrong": "She is play the piano.",
          "correct": "She is playing the piano.",
          "explanation": "be动词后面要用动词的-ing形式"
        },
        {
          "wrong": "I am knowing the answer.",
          "correct": "I know the answer.",
          "explanation": "know是状态动词，一般不用进行时"
        }
      ],
      "examples": [
        "They are watching TV at the moment.",
        "Look! The baby is crying.",
        "We are having an English class now."
      ],
      "sub_topics": [],
      "category": "时态",
      "difficulty": 2,
      "enabled": true,
      "is_new": false,
      "created_at": "2026-01-14 02:03:24",
      "updated_at": "2026-01-14 02:03:24",
      "_source": "database",
      "_matchScore": 1
    },
    {
      "id": 2,
      "title": "一般过去时",
      "keywords": [
        "一般过去时",
        "过去式",
        "did",
        "v.-ed",
        "was",
        "were",
        "simple past",
        "past tense"
      ],
      "definition": "表示过去某一时间发生的动作或存在的状态，也表示过去经常或反复发生的动作。",
      "structure": "主语 + 动词过去式（be动词用was/were）",
      "usage": [
        "表示过去发生的动作：I visited my grandma yesterday.",
        "表示过去存在的状态：He was a teacher ten years ago.",
        "表示过去的习惯：When I was young, I often played football."
      ],
      "mistakes": [
        {
          "wrong": "I go to Beijing last year.",
          "correct": "I went to Beijing last year.",
          "explanation": "有过去时间标志词（last year），动词要用过去式"
        },
        {
          "wrong": "Did you went there?",
          "correct": "Did you go there?",
          "explanation": "did后面的动词要用原形"
        }
      ],
      "examples": [
        "She bought a new dress yesterday.",
        "They were happy at the party.",
        "I didn't watch TV last night."
      ],
      "sub_topics": [],
      "category": "时态",
      "difficulty": 2,
      "enabled": true,
      "is_new": false,
      "created_at": "2026-01-14 02:03:24",
