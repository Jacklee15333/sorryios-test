  "grammar": [
    {
      "title": "动名词作整体概念使用",
      "definition": "动词加-ing形式表示一件事情或一种行为，用来指代整体概念。",
      "structure": "verb + -ing",
      "usage": [
        "用于表示一件事或一种行为",
        "可整体指代某种活动"
      ],
      "mistakes": [
        "将动名词当作普通动词随意使用"
      ],
      "examples": [
        "Protecting sth. is meaningful.",
        "Saving energy helps us."
      ]
    },
    {
      "title": "第三人称单数的一致关系",
      "definition": "当描述单一整体概念时，相关变化形式需保持一致。",
      "structure": "单一整体 + 相应变化形式",
      "usage": [
        "表示一件事而非多个行为",
        "强调整体性"
      ],
      "mistakes": [
        "误以为包含多个词就表示复数"
      ],
      "examples": [
        "Protecting sth. becomes important."
      ]
    },
    {
      "title": "比较程度的递进表达",
      "definition": "表示程度不断加深的含义。",
      "structure": "more and more + adj.",
      "usage": [
        "强调变化趋势",
        "多用于描述发展过程"
      ],
      "mistakes": [
        "误用为单纯比较级"
      ],
      "examples": [
        "The environment is getting more and more serious."
      ]
    },
    {
      "title": "句子连接的完整性",
      "definition": "表达多个意思时，需要明确关系并保持表达完整。",
      "structure": "句子 + 连接手段 + 句子",
      "usage": [
        "用于表达因果、并列或递进关系"
      ],
      "mistakes": [
        "直接用逗号连接两个完整意思"
      ],
      "examples": [
        "We should protect sth., and it can make life better."
      ]
    },
    {
      "title": "代词指代的明确性",
      "definition": "代词需要有清晰的指代对象，避免歧义。",
      "structure": "代词 + 所指内容",
      "usage": [
        "指代前文提到的内容"
      ],
      "mistakes": [
        "代词指代不清"
      ],
      "examples": [
        "This can help us.",
        "It makes life better."
      ]
    },
    {
      "title": "tell / say / speak / talk 的区别",
      "definition": "四个词都与“说”有关，但侧重点不同。",
      "structure": "",
      "usage": [
        "tell 强调把信息告诉某人",
        "say 强调说的内容",
        "speak 强调用语言或说话方式",
        "talk 强调交流过程"
      ],
      "mistakes": [
        "混用 speak 和 say",
        "talk 后直接接具体内容"
      ],
      "examples": [
        "He told sb. the news.",
        "She said it was true.",
        "He speaks English.",
        "They talked with sb."
      ]
    },
    {
      "title": "过去式与过去分词的功能区别",
      "definition": "两种形式用于不同表达场景，功能不同。",
      "structure": "",
      "usage": [
        "过去式用于描述过去发生的事情",
        "过去分词用于表示结果或状态"
      ],
      "mistakes": [
        "将过去分词当作过去式单独使用"
      ],
      "examples": [
        "I ate rice yesterday.",
        "I have eaten rice."
      ]
    },
    {
      "title": "现在完成表达结果",
      "definition": "用于表示已经完成并与现在有关的情况。",
      "structure": "",
      "usage": [
        "强调动作已完成",
        "关注结果对现在的影响"
      ],
      "mistakes": [
        "误用过去式代替"
      ],
      "examples": [
