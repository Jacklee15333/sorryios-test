  "grammar": [
    {
      "id": 8,
      "title": "被动语态",
      "keywords": [
        "被动语态",
        "be done",
        "被动",
        "by",
        "passive voice",
        "被动形式"
      ],
      "definition": "当主语是动作的承受者时，使用被动语态。强调动作的承受者或动作本身。",
      "structure": "主语 + be + 过去分词 + (by + 动作执行者)",
      "usage": [
        "不知道或不必说明动作执行者：The window was broken.",
        "强调动作承受者：The book was written by Lu Xun.",
        "客观报道或正式场合：The meeting will be held tomorrow.",
        "各种时态的被动：is done, was done, will be done, has been done等"
      ],
      "mistakes": [
        {
          "wrong": "The car was repair yesterday.",
          "correct": "The car was repaired yesterday.",
          "explanation": "be后面要用动词的过去分词"
        },
        {
          "wrong": "The book is belonged to me.",
          "correct": "The book belongs to me.",
          "explanation": "belong是不及物动词，没有被动语态"
        }
      ],
      "examples": [
        "English is spoken all over the world.",
        "The bridge was built in 1990.",
        "The work must be finished on time."
      ],
